<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_see_tree" model="ir.ui.view">
        <field name="name">catalogo.see.tree</field>
        <field name="model">catalogo.see</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <record id="action_catalogo_see" model="ir.actions.act_window">
        <field name="name">To See</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">catalogo.see</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="help" type="xml">
            <p class="o_view_nocontent_smiling_face">
                Create your list
            </p>
        </field>
    </record>

</odoo>

